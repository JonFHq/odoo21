<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="envio_action" model="ir.actions.act_window">
        <field name="name">envio</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">envio</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="envio_view_form" model="ir.ui.view">
        <field name="name">envio</field>
        <field name="model">envio</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <!--<field name="imagen" widget="image" class="oe_avatar"/>-->
                    <group>
                        <field name="origen" />
                        <field name="destino" />
                        <field name="estado" />
                        <field name="compañia" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="envio_view_tree" model="ir.ui.view">
        <field name="name">envio</field>
        <field name="model">envio</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="modelo" />
                <field name="imagen" />
                <field name="resolucion" />
                <field name="frecuencia" />
                <field name="tamaño" />
                <field name="peso" />
                <field name="RAM" />
                <field name="HDD" />
                <field name="SSD" />
                <field name="grafica" />
                <field name="USB3" />
                <field name="USB2" />
                <field name="placa_base" />
                <field name="lector_discos" />
                <field name="tactil" />
                <field name="tablet" />
                <field name="wifi" />
                <field name="cable_red" />
                <field name="bluetooth" />
                <field name="HDMI" />
                <field name="VGA" />
                <field name="DPORT" />
                <field name="miniDPORT" />
                <field name="thunderbolt" />
                <field name="jack" />
                <field name="SIM" />
                <field name="cantidad" />
                <field name="marca" />
            </tree>
        </field>
    </record>

</odoo>